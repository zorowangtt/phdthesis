  keywords = {\#nosource}
}

@article{xu2014a,
  title = {Vibrational {{Energy Flow}} through the {{Green Fluorescent Protein}}–{{Water Interface}}: {{Communication Maps}} and {{Thermal Boundary Conductance}}},
  shorttitle = {Vibrational {{Energy Flow}} through the {{Green Fluorescent Protein}}–{{Water Interface}}},
  author = {Xu, Yao and Leitner, David M.},
