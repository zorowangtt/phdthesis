@article{wei2019,
	title = {Chain length effect on thermal transport in amorphous polymers and a structure–thermal conductivity relation},
	volume = {21},
	issn = {1463-9076, 1463-9084},
	url = {http://xlink.rsc.org/?DOI=C9CP02397F},
	doi = {10.1039/C9CP02397F},
	abstract = {The physics of thermal transport in polymers is important in many applications, such as in heat exchangers and electronics packaging.
          , 
            
